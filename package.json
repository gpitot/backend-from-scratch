{
  "name": "express-es6-sample",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run prod",
    "dev": "NODE_ENV=development npm run watch ./src/bin/www",
    "prod": "NODE_ENV=production node ./src/bin/www",
    "build": "npm run clean && babel ./src --out-dir src/src",
    "deploy": "npm run clean && bash deploy",
    "clean": "rimraf src",
    "watch": "nodemon --config ./nodemon.json --"
  },
  "dependencies": {
    "aws-sdk": "^2.514.0",
    "bigint-hash": "^0.2.2",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "morgan": "~1.9.0",
    "node-fetch": "^2.6.0",
    "rimraf": "^2.6.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.4.4",
    "eslint": "^5.16.0",
    "eslint-plugin-import": "^2.17.2",
    "gulp": "^4.0.2",
    "gulp-zip": "^5.0.0",
    "nodemon": "^1.19.0",
    "yargs": "^14.0.0"
  }
}
